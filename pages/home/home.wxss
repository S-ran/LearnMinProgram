}
.box{
  width: 100%;
  height: 100rpx;
  background-color: #ccc;
  position: relative;
}
.active{
  width: 90%;
  height: 60rpx;
  background-color: #fff;
  position: absolute;
  top: 20rpx;
  left: 35rpx;
  text-align: center;
  border-radius: 10rpx;
}
.active1{
  width: 80%;
  height: 80rpx;
  background-color: #fff;
  position: absolute;
  top: 10rpx;
  left: 10rpx;
  border-radius: 10rpx;
}
.active2{
  position: absolute;
  top: 10rpx;
  right: 25rpx;
  font-size: 50rpx;
  color: green;
}
.cl{
  width: 40rpx;
  height: 40rpx;
  background: #ccc;
  position: absolute;
  top: 30rpx;
  right: 150rpx;
  border-radius: 50%;
